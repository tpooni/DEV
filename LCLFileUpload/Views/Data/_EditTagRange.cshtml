<hr style="color:#c00;background-color:#c00;height:1px;border:none;" />
<h4> @ViewBag.Message</h4>
<br />
<div>
    <h5>@Html.ActionLink("Back to List", "EnterTagRanges", new { JobNo = Request.Params["JobNo"] })</h5>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
